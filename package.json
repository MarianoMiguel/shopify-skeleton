{
  "name": "generator-shop-skeleton",
  "version": "1.0.0",
  "description": "scaffold and develop shopify themes quickly",
  "author": "Miguel Palau <mpalauzarza@gmail.com>",
  "scripts": {
    "copy": "node lib/copy",
    "clean": "node lib/clean",
    "compile": " babel -d generators/ --ignore templates/ generators/",
    "get-ready": "npm run clean; npm run copy; npm run compile",
    "prepublish": "npm run get-ready",
    "test": "mocha --require babel-register"
  },
  "files": [
    "generators/app"
  ],
  "keywords": [
    "yeoman-generator",
    "babel",
    "shopify"
  ],
  "repository": "git@code.pixel2html.com:development-tools/shopify-skeleton.git",
  "license": "MIT",
  "dependencies": {
    "babel-plugin-add-module-exports": "^0.2.1",
    "chalk": "^2.0.1",
    "cowsay-browser": "^1.1.8",
    "yeoman-generator": "^1.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-env": "^1.6.0",
    "babel-register": "^6.24.1",
    "fs-extra": "^3.0.1",
    "mocha": "^3.4.2",
    "standard": "^10.0.2",
    "yeoman-assert": "^3.0.0",
    "yeoman-test": "^1.7.0"
  },
  "standard": {
    "globals": [
      "describe",
      "it",
      "before",
      "beforeEach",
      "after",
      "afterEach",
      "xdescribe",
      "xit"
    ]
  }
}
